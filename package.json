{
  "name": "transform-jsx",
  "version": "0.1.0",
  "description": "Transform JSX into vanilla JavaScript",
  "author": "Ayushman Chhabra <ayushmxn@outlook.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/ayushmxn/transform-jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmxn/transform-jsx.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "demo": "esbuild ./test/demo/index.tsx --bundle --outfile=./test/demo/index.js && serve ./test/demo",
    "lint": "ts-standard ./src/*.ts --fix",
    "test": "jest"
  },
  "keywords": ["JSX", "JavaScript"],
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.16",
    "esbuild": "^0.14.21",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "serve": "^13.0.2",
    "ts-jest": "^27.1.3",
    "ts-standard": "^11.0.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "jsdom"
  }
}
