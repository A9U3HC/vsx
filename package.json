{
  "name": "vsx",
  "version": "0.8.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayushman Chhabra",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "lint": "eslint --config=.github/eslint.config.cjs ./src/**/*.ts",
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest run --coverage.enabled true"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uuid": "^11.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitest/coverage-v8": "^2.1.4",
    "esbuild": "~0.24.0",
    "eslint": "^8.57.1",
    "jsdom": "^25.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "volta": {
    "node": "20.18.0",
    "npm": "10.9.0"
  },
  "workspaces": [
    "templates/*"
  ]
}
