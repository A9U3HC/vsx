{
  "name": "vsx",
  "version": "0.7.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayushman Chhabra",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "lint": "eslint --config=.github/eslint.config.cjs ./src/**/*.ts",
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "esbuild": "~0.20.2",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": "20.12.1"
  },
  "packageManager": "npm@10.5.1",
  "workspaces": [
    "templates/*"
  ]
}
