{
  "name": "transform-jsx",
  "version": "0.2.1",
  "description": "Transform JSX into vanilla JavaScript",
  "author": "Ayushman Chhabra <ayushmxn@outlook.com>",
  "license": "MIT",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "homepage": "https://github.com/ayushmxn/transform-jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmxn/transform-jsx.git"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --format=esm --platform=node --target=node14 --outdir=bin && tsc --emitDeclarationOnly --outDir bin",
    "demo": "esbuild ./test/demo/index.tsx --bundle --format=esm --platform=node --outfile=./test/demo/index.js && serve ./test/demo",
    "lint": "ts-standard ./src/*.ts --fix",
    "test": "jest"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.12",
    "@typescript-eslint/parser": "^4.0.0",
    "esbuild": "^0.14.23",
    "eslint": "^7.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "serve": "^13.0.2",
    "ts-jest": "^27.1.3",
    "ts-standard": "^11.0.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "jsdom"
  }
}
