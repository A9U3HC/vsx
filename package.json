{
  "name": "vsx",
  "version": "0.8.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayushman Chhabra",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest run --coverage.enabled true"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "esbuild": "~0.24.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.4",
    "vitest": "^2.1.8"
  },
  "volta": {
    "node": "22.12.0",
    "npm": "11.0.0"
  },
  "workspaces": [
    "templates/*"
  ]
}
