{
  "name": "transform-jsx",
  "version": "0.3.2-beta.0",
  "description": "Write reactive vanilla JavaScript in JSX.",
  "author": "Ayushman Chhabra <ayushmxn@outlook.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/ayushmxn/transform-jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmxn/transform-jsx.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "demo": "pnpm build && esbuild ./test/demo/index.tsx --bundle --platform=node --outfile=./test/demo/index.js && serve ./test/demo",
    "lint": "ts-standard ./src/*.ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uniqid": "5.4.0"
  },
  "devDependencies": {
    "@types/jest": "27.5.1",
    "@types/node": "17.0.32",
    "@typescript-eslint/parser": "^4.0.0",
    "esbuild": "0.14.39",
    "eslint": "^7.0.0",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jsdom": "19.0.0",
    "serve": "13.0.2",
    "ts-jest": "28.0.2",
    "ts-standard": "11.0.0",
    "tsup": "5.11.13",
    "typescript": "4.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "jsdom"
  }
}
