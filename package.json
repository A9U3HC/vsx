{
  "name": "vsx",
  "version": "0.7.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayush C.",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "lnt": "eslint --config=.github/eslint.config.cjs ./src/**/*.ts",
    "bld": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest",
    "demo": "npm run bld && esbuild ./test/demo/index.tsx --bundle --platform=node --outdir=./test/demo --watch --servedir=./test/demo"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "esbuild": "~0.20.1",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "packageManager": "npm@10.4.0"
}
