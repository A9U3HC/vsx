{
  "name": "transform-jsx",
  "version": "0.3.1-beta.1",
  "description": "Transform JSX into vanilla JavaScript",
  "author": "Ayushman Chhabra <ayushmxn@outlook.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "./dist"

  ],
  "homepage": "https://github.com/ayushmxn/transform-jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmxn/transform-jsx.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "demo": "pnpm build && esbuild ./test/demo/index.tsx --bundle --platform=node --outfile=./test/demo/index.js && serve ./test/demo",
    "lint": "ts-standard ./src/*.ts --fix",
    "test": "jest"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^14.18.12",
    "@typescript-eslint/parser": "^4.0.0",
    "esbuild": "^0.14.23",
    "eslint": "^7.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "serve": "^13.0.2",
    "ts-jest": "^27.1.3",
    "ts-standard": "^11.0.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "uniqid": "^5.4.0"
  }
}
