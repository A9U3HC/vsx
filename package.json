{
  "name": "vsx",
  "version": "0.7.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayush C.",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "fmt": "prettier --write ./src ./test",
    "lnt": "eslint --config=.github/eslint.config.cjs ./src/**/*.ts",
    "bld": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest",
    "demo": "npm run bld && esbuild ./test/demo/index.tsx --bundle --platform=node --outdir=./test/demo --watch --servedir=./test/demo"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "engines": {
    "node": ">= 20.10.0",
    "npm": ">= 10.2.4"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "esbuild": "~0.19.8",
    "eslint": "^8.55.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "serve": "^14.2.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
